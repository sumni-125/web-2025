--1.  블로그 업로드 할 때 사용한 테이블
CREATE table blog_upload_tbl(
     blog_id varchar2(20) primary KEY, -- 블로그 아이디	
     title varchar2(100), -- 블로그 제목 입력할 곳
     content varchar(1000), -- 블로그 내용 입력할 곳
     regdate DATE , -- 날짜를 나타낼 곳
     location_id varchar2(20) REFERENCES location_info_tbl(location_id),
     USER_CODE VARCHAR2(50) REFERENCES TRIPMATE_USER(USER_CODE)
);

-- 새로운 시퀀스 생성
CREATE SEQUENCE blog_upload_seq
START WITH 100
INCREMENT BY 1
NOCACHE;

-- 트리거 생성
CREATE OR REPLACE TRIGGER trg_blog_upload_id
BEFORE INSERT ON blog_upload_tbl
FOR EACH ROW
BEGIN
    IF :NEW.blog_id IS NULL THEN
        SELECT 'B' || LPAD(blog_upload_seq.NEXTVAL, 3, '0')
        INTO :NEW.blog_id
        FROM DUAL;
    END IF;
END;

-- 1. 블로그 업로드 insert (3)
INSERT INTO blog_upload_tbl (title, content, regdate, location_id, USER_CODE)
VALUES ('전주 여행', '신나는 전주 여행을 다녀 왔다', sysdate, 'L063', 'U00003');

INSERT INTO blog_upload_tbl (title, content, regdate, location_id, USER_CODE)
VALUES ('부산 여행', '신나는 부산 여행을 다녀 왔다', sysdate, 'L051', 'U00003');

-- 2. 이미지 정보 테이블
CREATE table image_info_tbl(
     image_id varchar2(20) PRIMARY KEY, -- 이미지 아이디
     image_url varchar2(100), -- 이미지 주소
     blog_id varchar2(20) REFERENCES blog_upload_tbl(blog_id)
);

CREATE SEQUENCE image_info_seq
START WITH 10
INCREMENT BY 1
NOCACHE;

CREATE OR REPLACE TRIGGER trg_image_info_id
BEFORE INSERT ON image_info_tbl
FOR EACH ROW
BEGIN
    IF :NEW.image_id IS NULL THEN
        SELECT 'I' || LPAD(image_info_seq.NEXTVAL, 3, '0')
        INTO :NEW.image_id
        FROM DUAL;
    END IF;
END;

-- 5. 검색 상세 정보 테이블 (관광지)
CREATE TABLE loc_img_info(
	img_title varchar2(100),
	img_content varchar2(4000),
	img_url varchar2(100),
	location_address varchar2(1000),
	location_id varchar2(20) REFERENCES location_info_tbl(location_id)
);

-- 6. 호텔 정보 테이블
CREATE TABLE loc_hotel_info(
	hotel_title varchar2(100),
	hotel_rating number(5),
	hotel_price number(20),
	location_address varchar2(1000),
	img_url varchar2(100),
	location_id varchar2(20) REFERENCES location_info_tbl(location_id)
);

