INSERT INTO trip_member_tbl (SD_CODE, USER_CODE) VALUES ('T00002', 'U00004');

-- 고정데이터 테이블들
-- 카테고리(숙박, 항공, 교통, 식사, 관광, 쇼핑, 기타)
CREATE TABLE category_tbl (
    category_code VARCHAR2(20) PRIMARY KEY
);

-- 카테고리 고정 데이터
INSERT INTO category_tbl VALUES ('숙박');
INSERT INTO category_tbl VALUES ('항공');
