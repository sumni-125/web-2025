
-- 외부 데이터 테이블들(다른쪽에서 만들어져야 되는 테이블) 
-- 유저 테이블 시쿼스 포함 건들지 말 것
CREATE TABLE TRIPMATE_USER (
    USER_CODE VARCHAR2(50) PRIMARY KEY,             -- 유저 고유번호
    ID VARCHAR2(12 CHAR) NOT NULL UNIQUE,           -- 유저 아이디
    PW VARCHAR2(18 CHAR) NOT NULL,                  -- 유저 비밀번호
    NICKNAME VARCHAR2(10 CHAR) NOT NULL UNIQUE      -- 유저 닉네임(= 이름)
);

CREATE SEQUENCE SEQ_USER_CODE
START WITH 1
INCREMENT BY 1
NOCACHE;

CREATE OR REPLACE TRIGGER TRG_USER_CODE
BEFORE INSERT ON TRIPMATE_USER
FOR EACH ROW
BEGIN
    IF :NEW.USER_CODE IS NULL THEN
        SELECT 'U' || LPAD(SEQ_USER_CODE.NEXTVAL, 5, '0') INTO :NEW.USER_CODE FROM DUAL;
    END IF;
END;
/

-- 유저 데이터(임시 데이터)
INSERT INTO TRIPMATE_USER (ID, PW, NICKNAME)
VALUES ('testuser', 'password123', '테스트');
