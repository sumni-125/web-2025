VALUES ('T00002', 'day1', '서울 마포구 서교동 357-1', 37.553619, 126.921607, 2, 'test');

-- 여행스케줄(기간) 테이블
CREATE TABLE TRIPMATE_SCHEDULE (
    SD_CODE VARCHAR2(50) PRIMARY KEY,       -- 여행 고유키
    NAME VARCHAR2(20) NOT NULL,             -- 일정 제목
    START_DATE DATE NOT NULL,               -- 시작일
    END_DATE DATE NOT NULL,                 -- 종료일
    PLACE_NAME VARCHAR2(100) NOT NULL       -- 여행 지역명
);

CREATE SEQUENCE SEQ_SD_CODE
START WITH 1
INCREMENT BY 1
NOCACHE;

CREATE OR REPLACE TRIGGER TRG_SD_CODE
BEFORE INSERT ON TRIPMATE_SCHEDULE
FOR EACH ROW
BEGIN
    IF :NEW.SD_CODE IS NULL THEN
        SELECT 'T' || LPAD(SEQ_SD_CODE.NEXTVAL, 5, '0') INTO :NEW.SD_CODE FROM DUAL;
    END IF;
END;
/

select * from TRIPMATE_SCHEDULE;
-- 여행 스케쥴 데이터(임시 데이터)
INSERT INTO TRIPMATE_SCHEDULE (NAME, START_DATE, END_DATE, PLACE_NAME)
VALUES ('제주도 여행', TO_DATE('2025-04-10', 'YYYY-MM-DD'), TO_DATE('2025-04-18', 'YYYY-MM-DD'), '제주도');

INSERT INTO TRIPMATE_SCHEDULE (NAME, START_DATE, END_DATE, PLACE_NAME)
VALUES ('제주도 여행', TO_DATE('2025-03-10', 'YYYY-MM-DD'), TO_DATE('2025-03-18', 'YYYY-MM-DD'), '제주도');



-- 여행 참여자 테이블
CREATE TABLE trip_member_tbl (
    sd_code   VARCHAR2(50) NOT NULL REFERENCES TRIPMATE_SCHEDULE(SD_CODE),   -- 외래키: 여행 고유키
    user_code VARCHAR2(50) NOT NULL REFERENCES TRIPMATE_USER(USER_CODE),       -- 외래키: 유저 고유키
    
    CONSTRAINT pk_trip_member PRIMARY KEY (SD_CODE, user_code)            -- 복합 기본키 설정
);

-- 여행 참여자(임시 데이터)
INSERT INTO trip_member_tbl (SD_CODE, USER_CODE) VALUES ('T00001', 'U00001');
