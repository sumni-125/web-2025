VALUES ('test2', 'test2', '테스트4');

select * from TRIPMATE_marker_data;
update TRIPMATE_marker_data SET days = 'day1', address = '서울 마포구 서교동 345-1', lat = 37.655215, lng = 126.923506, day_order = 1, descriptionTEXT = 'test1' WHERE SD_CODE = 'T00002' and marker_code = 'M000001';
CREATE TABLE TRIPMATE_marker_data (
    marker_code VARCHAR2(50) PRIMARY KEY,
    sd_code     VARCHAR2(50) REFERENCES TRIPMATE_SCHEDULE(SD_CODE),       -- Schedule code (외래키 등으로 활용 가능)
    dayS         VARCHAR2(10),       -- 예: 'day1', 'day2'
    address     VARCHAR2(255),      -- 장소 주소
    lat         NUMBER(10, 6),      -- 위도 (정밀도: 소수점 이하 6자리)
    lng         NUMBER(10, 6),      -- 경도
    day_order   NUMBER(3),          -- 당일 순서
    descriptionTEXT VARCHAR2(500)      -- 부가 설명
);

CREATE SEQUENCE seq_marker_code
START WITH 1
INCREMENT BY 1
NOCACHE
NOCYCLE;

CREATE OR REPLACE TRIGGER trg_marker_code
BEFORE INSERT ON TRIPMATE_marker_data
FOR EACH ROW
BEGIN
    :NEW.marker_code := 'M' || TO_CHAR(seq_marker_code.NEXTVAL, 'FM000000'); -- 'M'을 앞에 붙여서 코드 형식 지정
END;
/

SELECT * FROM TRIPMATE_marker_data;
INSERT into TRIPMATE_marker_data(SD_CODE, dayS, address, lat, lng, day_order, descriptionTEXT)
VALUES ('T00002', 'day1', '서울 마포구 서교동 345-1', 37.555215, 126.923506, 1, 'test');

