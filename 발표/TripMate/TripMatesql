INSERT INTO category_tbl VALUES ('기타');

-- 결제수단(카드결제, 현금결제)
CREATE TABLE payment_type_tbl (
    type_code VARCHAR2(20) PRIMARY KEY
);

-- 결제 수단 고정 데이터
INSERT INTO payment_type_tbl VALUES ('카드결제');
INSERT INTO payment_type_tbl VALUES ('현금결제');

-- 결제 방식(1/N, 한번에결제, 개인결제)
CREATE TABLE payment_method_tbl (
    method_code VARCHAR2(20) PRIMARY KEY
);

-- 결제 방식 고정 데이터
INSERT INTO payment_method_tbl VALUES ('1/N');
INSERT INTO payment_method_tbl VALUES ('한번에결제');
INSERT INTO payment_method_tbl VALUES ('개인결제');



-- 실제 데이터가 입력되는 테이블들
-- 지출 데이터 테이블
CREATE TABLE cost_tbl (
    cost_id        VARCHAR2(50) PRIMARY KEY,                                        -- 자동생성(결제 고유키)
    trip_code      VARCHAR2(50) NOT NULL REFERENCES TRIPMATE_SCHEDULE(SD_CODE),        -- 외부 여행테이블의 고유키(외래키)
    trip_date      DATE NOT NULL,                                                   -- 여행 날짜(지출이 발생한 날짜)
    category_code  VARCHAR2(20) NOT NULL REFERENCES category_tbl(category_code),    -- 카테고리(고정테이블)
    location       VARCHAR2(150) NOT NULL,                                          -- 장소
    amount         NUMBER NOT NULL,                                                 -- 총 금액
    payment_type   VARCHAR2(20) NOT NULL REFERENCES payment_type_tbl(type_code),    -- 결제 수단(고정테이블)
    payment_method VARCHAR2(20) NOT NULL REFERENCES payment_method_tbl(method_code),-- 결제 방식(고정테이블)
    payer_code     VARCHAR2(50) NOT NULL REFERENCES TRIPMATE_USER(user_code)           -- 결제자(유저 테이블의 유저이름을 가져오기 위한 외래키)
);

CREATE SEQUENCE SEQ_COST_ID
START WITH 1
INCREMENT BY 1
NOCACHE;

CREATE OR REPLACE TRIGGER TRG_COST_ID
BEFORE INSERT ON cost_tbl
FOR EACH ROW
BEGIN
    IF :NEW.cost_id IS NULL THEN
        SELECT 'C' || LPAD(SEQ_COST_ID.NEXTVAL, 5, '0') INTO :NEW.cost_id FROM DUAL;
    END IF;
END;
/


-- 개인(참여자) 비용 테이블
CREATE TABLE personal_cost_tbl (
    personal_cost_id VARCHAR2(50) PRIMARY KEY,                                      -- 자동생성 (개인 지불 고유키)
    cost_id          VARCHAR2(50) NOT NULL REFERENCES cost_tbl(cost_id),            -- 외래키: 어떤 지출에서 나왔는지
    user_code        VARCHAR2(50) NOT NULL REFERENCES TRIPMATE_USER(user_code),        -- 유저 외래키(유저 테이블의 유저이름을 가져오기 위한 외래키)
    personal_cost    NUMBER NOT NULL,                                               -- 개인 할당 금액
    status           VARCHAR2(20) NOT NULL CHECK (status IN ('정산대기중', '정산완료'))-- 정산 상태(정산대기중, 정산완료)
);

CREATE SEQUENCE SEQ_PERSONAL_COST_ID
START WITH 1
INCREMENT BY 1
NOCACHE;

CREATE OR REPLACE TRIGGER TRG_PERSONAL_COST_ID
BEFORE INSERT ON personal_cost_tbl
FOR EACH ROW
BEGIN
    IF :NEW.personal_cost_id IS NULL THEN
        SELECT 'P' || LPAD(SEQ_PERSONAL_COST_ID.NEXTVAL, 5, '0') INTO :NEW.personal_cost_id FROM DUAL;
    END IF;
END;
/
commit;





select * from cost_tbl;

select * from personal_cost_tbl;





select * from TRIPMATE_SCHEDULE;

select * from TRIPMATE_USER;

select * from trip_member_tbl;




-- 1) 트리거 삭제
DROP TRIGGER TRG_PERSONAL_COST_ID;
DROP TRIGGER TRG_COST_ID;
DROP TRIGGER TRG_SD_CODE;
DROP TRIGGER TRG_USER_CODE;

-- 2) 시퀀스 삭제
DROP SEQUENCE SEQ_PERSONAL_COST_ID;
DROP SEQUENCE SEQ_COST_ID;
DROP SEQUENCE SEQ_SD_CODE;
DROP SEQUENCE SEQ_USER_CODE;

-- 3) 테이블 삭제 (CASCADE CONSTRAINTS 옵션으로 외래키 제약조건도 함께 제거)
DROP TABLE personal_cost_tbl CASCADE CONSTRAINTS;
DROP TABLE cost_tbl           CASCADE CONSTRAINTS;
DROP TABLE trip_member_tbl    CASCADE CONSTRAINTS;
DROP TABLE payment_method_tbl CASCADE CONSTRAINTS;
DROP TABLE payment_type_tbl   CASCADE CONSTRAINTS;
DROP TABLE category_tbl       CASCADE CONSTRAINTS;
DROP TABLE TRIPMATE_SCHEDULE  CASCADE CONSTRAINTS;
DROP TABLE TRIPMATE_USER      CASCADE CONSTRAINTS;


-- 3. 지역 테이블 (1)
CREATE table location_info_tbl(
     location_id varchar2(20) PRIMARY KEY NOT NULL, -- 지역번호
     location_name varchar2(20) NOT NULL -- 이름
)

-- 4. 검색 메인 이미지 테이블 (2)
CREATE TABLE location_img_tbl(
	img_url varchar2(100),
	location_id varchar2(20) REFERENCES location_info_tbl(location_id)
)

